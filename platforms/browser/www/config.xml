<?xml version='1.0' encoding='utf-8'?>
<widget id="com.eclipse.sasscodestudio" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://eclipse.com/ns/1.1">
    <name>Eclipse</name>
    <description>A budget Planner Mobile App</description>
    <author email="support@eclipse.com" href="http://eclipse.com">Eclipse Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
